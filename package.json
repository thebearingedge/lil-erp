{
  "private": true,
  "name": "lil-erp",
  "version": "1.0.0",
  "description": "Inventory Data Model",
  "main": "index.js",
  "scripts": {
    "test": "mocha --harmony-async-await -r babel-register app/**/*.test.js",
    "tdd": "npm t -s -- -w -R min",
    "cover": "nyc npm t -s",
    "db": "babel-node --harmony-async-await node_modules/.bin/knex --cwd db/",
    "db:make": "npm run -s db migrate:make",
    "db:up": "npm run -s db migrate:latest",
    "db:down": "npm run -s db migrate:rollback",
    "db:cycle": "run-s -s db:down db:up",
    "db:seed": "npm run -s db seed:run",
    "precommit": "npm run cover"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thebearingedge/lil-erp.git"
  },
  "keywords": [],
  "author": "Tim Davis",
  "license": "LicenseRef-LICENSE",
  "bugs": {
    "url": "https://github.com/thebearingedge/lil-erp/issues"
  },
  "homepage": "https://github.com/thebearingedge/lil-erp#readme",
  "devDependencies": {
    "babel-cli": "6.18.0",
    "babel-eslint": "^7.1.1",
    "babel-plugin-transform-es2015-modules-commonjs": "6.18.0",
    "babel-plugin-transform-export-extensions": "6.8.0",
    "babel-plugin-transform-object-rest-spread": "6.20.2",
    "bluebird": "^3.4.6",
    "chai": "3.5.0",
    "chai-struct": "2.1.1",
    "eslint": "^3.11.1",
    "husky": "0.12.0",
    "knex": "0.12.6",
    "mocha": "3.2.0",
    "npm-run-all": "^3.1.2",
    "nyc": "10.0.0"
  },
  "dependencies": {
    "deep-clone": "3.0.2",
    "fs-promise": "1.0.0",
    "global": "4.3.1",
    "knex": "0.12.6",
    "lodash": "4.17.3",
    "pg": "6.1.2"
  }
}
