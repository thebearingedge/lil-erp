  "bugs": {
    "url": "https://github.com/thebearingedge/inventory/issues"
  },
  "homepage": "https://github.com/thebearingedge/inventory#readme",
  "devDependencies": {
    "babel-cli": "6.18.0",
    "babel-plugin-transform-es2015-modules-commonjs": "6.18.0",
    "chai": "3.5.0",
    "chai-struct": "2.1.0",
    "mocha": "3.2.0"
  },
  "dependencies": {
    "bluebird": "3.4.6",
    "knex": "0.12.6",
    "pg": "6.1.0"
  }
