{
  "private": true,
  "name": "inventory",
  "version": "1.0.0",
  "description": "Inventory Data Model",
  "main": "index.js",
  "scripts": {
    "test": "mocha --harmony-async-await -r babel-register",
    "tdd": "npm t -s -- -w -R min",
    "db": "babel-node --harmony-async-await node_modules/.bin/knex",
    "db:up": "npm run -s db migrate:latest",
    "db:down": "npm run -s db migrate:rollback",
    "db:make": "npm run -s db migrate:make",
    "db:seed": "npm run -s db seed:run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thebearingedge/inventory.git"
  },
